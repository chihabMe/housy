{
  "version": 2,
  "buildCommand": "pnpm db:migrate && npm api:build",
  "builds": [
    {
      "src": "dist/app.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"],

        "buildCommand": "pnpm db:migrate && npm api:build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/app.js"
    }
  ]
}
